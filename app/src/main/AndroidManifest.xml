<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.badknees.esssquad">

    <application
        android:allowBackup="true"
        android:icon="@drawable/payments256"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Market"
            android:label="@string/title_activity_market"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Payment"
            android:label="@string/title_activity_payment"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".AddCard"
            android:label="@string/title_activity_add_card"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".dialoglayout"
            android:label="@string/title_activity_dialoglayout"
            android:theme="@style/AppTheme">
        </activity>
    </application>

</manifest>